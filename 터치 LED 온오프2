int led = 2;
int states = 0; // 상태값을 0으로 초기화한다.

void setup() {
    pinMode(led, OUTPUT);
    Serial.begin(9600);
    
}

void loop() {

        if(touchRead(T0) <= 70){ // 87이 기본값이므로 70으로 한다.
          states=!states; // 상태를 반전시킨다.
        }
        digitalWrite(led, states);
          
        Serial.println(touchRead(T0));
        delay(1000); // 1초에 터치 1회가 안정적이다.

}

//states함수 참고
