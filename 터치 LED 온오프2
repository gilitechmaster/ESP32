int led1 = 2;
int led2 = 18;
int states = 0;

void setup() {
    pinMode(led1, OUTPUT);
    pinMode(led2, OUTPUT);
    Serial.begin(9600);
    
}

void loop() {

        if(touchRead(T0) <= 10){
          states=!states;
        }
        digitalWrite(led1, states);
        digitalWrite(led2, states);
          
        Serial.println(touchRead(T0));
        delay(1000);

}

//states함수 참고
