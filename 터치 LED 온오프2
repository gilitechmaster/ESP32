int led = 2;
int states = 0;

void setup() {
    pinMode(led, OUTPUT);
    Serial.begin(9600);
    
}

void loop() {

        if(touchRead(T0) <= 30){
          states=!states;
        }
        digitalWrite(led, states);
          
        Serial.println(touchRead(T0));
        delay(1000); // 1초에 터치 1회가 안정적이다.

}

//states함수 참고
